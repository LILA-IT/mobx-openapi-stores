{
  "name": "mobx-openapi-generator",
  "version": "0.1.0",
  "description": "A MobX-based store for OpenAPI generated clients",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "dev": "tsc -p tsconfig.esm.json -w --preserveWatchOutput",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "to-flow-generator-function": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "mobx": ">=6.0.0"
  },
  "author": "Your Name <you@example.com>",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mobx",
    "openapi",
    "store",
    "typescript"
  ]
}
